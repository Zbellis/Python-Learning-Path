    "X = DataFrame(df, columns=['distance'])\n",
    "y = DataFrame(df, columns=['pace'])\n",
    "plt.figure(figsize=(10,6))\n",
    "plt.scatter(X, y, alpha=0.5)\n",
    "plt.title('Pace Analysis: Distance over Minutes per Mile', fontsize=17)\n",
    "plt.xlabel('Distance (mi)', fontsize=16)\n",
    "plt.ylabel('Minutes Per Mile', fontsize=16)\n",
    "plt.ylim(5, 10)\n",
    "plt.xlim(0, 30)\n",
    "plt.show()"
   ]
